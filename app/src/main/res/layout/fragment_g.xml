<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFAE5F"
    tools:context="com.wsoteam.diet.InApp.Fragments.FragmentG">


    <ImageView
        android:id="@+id/imageView109"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_img_onb_4" />

    <ImageView
        android:id="@+id/imageView110"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:src="@drawable/ic_fiasy_premium"
        app:layout_constraintBottom_toTopOf="@+id/imageView109"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/premiumGClose"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/ic_cross_premium"
        app:layout_constraintBottom_toTopOf="@+id/imageView109"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider2"
        android:layout_width="434dp"
        android:layout_height="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="100dp"
        android:background="?android:attr/listDivider"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView109"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewPremG"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider2">

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/prem_g_main_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:elevation="10dp"
        android:textAlignment="center"
        android:textColor="@color/black_for_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/cardViewPremG"
        app:layout_constraintTop_toTopOf="@+id/cardViewPremG"
        tools:text="TextView" />

    <TextView
        android:id="@+id/premGField1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:drawableStart="@drawable/ic_checkmark"
        android:drawablePadding="16dp"
        android:elevation="10dp"
        android:fontFamily="@font/roboto"
        android:text="@string/premium_G_field_1"
        android:textColor="@color/black_for_txt"
        app:layout_constraintEnd_toEndOf="@+id/cardViewPremG"
        app:layout_constraintStart_toStartOf="@+id/cardViewPremG"
        app:layout_constraintTop_toBottomOf="@+id/prem_g_main_txt" />

    <TextView
        android:id="@+id/premGField2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:drawableStart="@drawable/ic_checkmark"
        android:drawablePadding="16dp"
        android:elevation="10dp"
        android:fontFamily="@font/roboto"
        android:text="@string/premium_G_field_2"
        android:textColor="@color/black_for_txt"
        app:layout_constraintEnd_toEndOf="@+id/cardViewPremG"
        app:layout_constraintStart_toStartOf="@+id/cardViewPremG"
        app:layout_constraintTop_toBottomOf="@+id/premGField1" />

    <TextView
        android:id="@+id/premGField3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:drawableStart="@drawable/ic_checkmark"
        android:drawablePadding="16dp"
        android:elevation="10dp"
        android:fontFamily="@font/roboto"
        android:text="@string/premium_G_field_3"
        android:textColor="@color/black_for_txt"
        app:layout_constraintEnd_toEndOf="@+id/cardViewPremG"
        app:layout_constraintStart_toStartOf="@+id/cardViewPremG"
        app:layout_constraintTop_toBottomOf="@+id/premGField2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buyPremiumG"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/unactive_promo_btn"
        android:text="@string/premium_g_button_txt"
        android:textColor="@color/white"
        app:backgroundTint="@color/pumpkin_orange"
        app:layout_constraintBottom_toBottomOf="@+id/cardViewPremG"
        app:layout_constraintEnd_toEndOf="@+id/cardViewPremG"
        app:layout_constraintStart_toStartOf="@+id/cardViewPremG"
        app:layout_constraintTop_toBottomOf="@+id/premGField3" />

</androidx.constraintlayout.widget.ConstraintLayout>